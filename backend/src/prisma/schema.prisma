generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model User {
  id       String @id @default(uuid())
  name     String
  email    String @unique
  password String
}

model Transcription {
  id      String  @id @default(uuid())
  rolesId String
  roles   Roles[]
}

model Roles {
  id              String        @id @default(uuid())
  name            String        @unique
  description     String
  transcriptionId String
  Transcription   Transcription @relation(fields: [transcriptionId], references: [id])
}
